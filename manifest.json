{
  "manifest_version": 2,

  "name": "Lingvo live Omnibox",
  "description": "Perform lingvolive.ru search from Omnibox, with suggestions",
  "version": "1.0",
 
  "offline_enabled": false,
  "options_page": "options.html",
  "omnibox": { "keyword" : "l" },
  "icons": {
    "16": "16.png",
    "128": "128.png"
  },
  "permissions": [
    "storage"
  ],
  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  }
}